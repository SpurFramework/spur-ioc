{
  "extends": "../.eslintrc",
  "env": {
    "es6": true,
    "mocha": true,
    "node": true
  },
  "globals": {
    "sinon": true,
    "injector": true,
    "nock": true,
    "_": true,
    "expect": true,
    "chai": true
  },
  "rules": {
    "no-unused-expressions": 0,
    "max-len": 0,
    "arrow-body-style": 0,
    "no-console": 0,
    "no-unused-vars": 0
  }
}
